<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jbcar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.jbcar.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jbcar.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.example.jbcar.CarsMainActivity"
            android:label="@string/title_activity_cars_main" >
        </activity>
        <activity
            android:name="com.example.jbcar.CarDetailsActivity"
            android:label="@string/title_activity_car_details" >
        </activity>
        <activity
            android:name="com.example.jbcar.CarSettingsActivity"
            android:label="@string/title_activity_car_settings" >
        </activity>
    </application>

</manifest>
